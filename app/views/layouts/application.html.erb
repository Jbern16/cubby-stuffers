<!DOCTYPE html>
<html>
  <head>
    <title>CubbyStuffers</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= render 'shared/navbar' %>

    <% flash.each do |name, message| %>
      <%= content_tag :div, raw(message), id: "flash_#{name}" %>
    <% end %>

    <div id='content'>
      <%= yield %>
    </div>
  </body>
</html>
